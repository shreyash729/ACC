#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t;
    cin>>t;
    while(t--){
        int N;
        cin>>N;
        int d = 0;
        while(N>0){
        int digit = N%10;
        d= digit+(10*d);
        N/=10;
        }
        cout<<d<<endl;
    }
    return 0;
}
